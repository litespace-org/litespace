{
  "name": "litespace",
  "version": "1.0.0",
  "description": "LiteSpace Monorepo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:pkgs": "pnpm assets build && pnpm types build && pnpm utils build && pnpm emails build && pnpm models build && pnpm atlas build && pnpm headless build && pnpm auth build &&  pnpm ui build && pnpm radio build && pnpm kafka build && pnpm tests build && pnpm schemas build",
    "build:server:pkgs": "pnpm types build && pnpm utils build && pnpm emails build && pnpm models build && pnpm atlas build && pnpm auth build && pnpm radio build && pnpm kafka build && pnpm tests build && pnpm schemas build",
    "watch:atlas": "pnpm atlas watch",
    "watch:auth": "pnpm auth watch",
    "watch:emails": "pnpm emails watch",
    "watch:ui": "pnpm ui watch",
    "watch:models": "pnpm models watch",
    "watch:utils": "pnpm utils watch",
    "watch:types": "pnpm types watch",
    "watch:headless": "pnpm headless watch",
    "watch:pkgs": "concurrently --names \"types,utils,atlas,headless,auth,email,ui,models\" \"pnpm watch:types\" \"pnpm watch:utils\"  \"pnpm watch:atlas\" \"pnpm watch:headless\" \"pnpm watch:auth\" \"pnpm watch:emails\" \"pnpm watch:ui\" \"pnpm watch:models\"",
    "dev:web": "pnpm --filter @litespace/web dev",
    "dev:dash": "pnpm --filter @litespace/dashboard dev",
    "dev:landing": "pnpm --filter @litespace/landing dev",
    "docker": "docker compose up -d",
    "docker:linux": "sudo docker compose up -d",
    "build:web:vercel": "pnpm --filter @litespace/web build:vercel",
    "build:ui:vercel": "pnpm --filter @litespace/ui build:vercel",
    "build:dashboard:vercel": "pnpm --filter @litespace/dashboard build:vercel",
    "deploy:orbit:vercel": "pnpm --filter @litespace/orbit deploy:vercel",
    "deploy:web:vercel": "pnpm --filter @litespace/web deploy:vercel",
    "deploy:ui:vercel": "pnpm --filter @litespace/ui deploy:vercel",
    "deploy:landing:vercel": "pnpm --filter @litespace/landing deploy:vercel",
    "deploy:dashboard:vercel": "pnpm --filter @litespace/dashboard deploy:vercel",
    "pre:push": "pnpm intl inspect; pnpm prettier:check; pnpm lint; pnpm check:all; pnpm web build; pnpm dashboard build",
    "pre:push:tests": "pnpm models test:local; pnpm run server test:handlers",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "check:all": "pnpm atlas check && pnpm auth check && pnpm emails check && pnpm headless check && pnpm ui check && pnpm models check && pnpm utils check && pnpm types check && pnpm radio check && pnpm kafka check && pnpm web check && pnpm dashboard check && pnpm tests check",
    "storybook": "pnpm --filter @litespace/ui storybook",
    "vercel": "env-cmd vite-node scripts/vercel.ts",
    "vercel:ci": "vite-node scripts/vercel.ts",
    "telegram": "vite-node scripts/telegram.ts",
    "release": "vite-node scripts/release.ts",
    "github": "vite-node scripts/github.ts",
    "intl": "vite-node scripts/intl.ts",
    "dangerfile": "danger --dangerfile scripts/dangerfile.ts",
    "clickup": "vite-node scripts/clickup.ts",
    "benchmark": "vite-node scripts/benchmark.ts",
    "clean:dep": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "clean:dist": "pnpm -r exec rm -rf dist",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "assets": "pnpm --filter @litespace/assets",
    "atlas": "pnpm --filter @litespace/atlas",
    "auth": "pnpm --filter @litespace/auth",
    "emails": "pnpm --filter @litespace/emails",
    "headless": "pnpm --filter @litespace/headless",
    "ui": "pnpm --filter @litespace/ui",
    "models": "pnpm --filter @litespace/models",
    "utils": "pnpm --filter @litespace/utils",
    "types": "pnpm --filter @litespace/types",
    "tests": "pnpm --filter @litespace/tests",
    "schemas": "pnpm --filter @litespace/schemas",
    "radio": "pnpm --filter @litespace/radio",
    "server": "pnpm --filter @litespace/server",
    "deployer": "pnpm --filter @litespace/deployer",
    "web": "pnpm --filter @litespace/web",
    "dashboard": "pnpm --filter @litespace/dashboard",
    "landing": "pnpm --filter @litespace/landing",
    "mobile": "pnpm --filter @litespace/mobile",
    "blog": "pnpm --filter @litespace/blog",
    "jobs": "pnpm --filter @litespace/jobs",
    "messenger": "pnpm --filter @litespace/messenger",
    "studio": "pnpm --filter @litespace/studio",
    "kafka": "pnpm --filter @litespace/kafka",
    "lens": "pnpm --filter @litespace/lens",
    "livekit:dev": "livekit-server --dev --config livekit.config.yaml"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.26.0",
    "@litespace/atlas": "workspace:^",
    "@litespace/types": "workspace:^",
    "@types/autocannon": "^7.12.5",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "autocannon": "^8.0.0",
    "axios": "^1.7.7",
    "commander": "^13.1.0",
    "concurrently": "^8.2.2",
    "danger": "^12.3.3",
    "env-cmd": "^10.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^11.0.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "prettier": "3.4.2",
    "pretty-bytes": "^6.1.1",
    "rimraf": "^5.0.7",
    "table": "^6.9.0",
    "ts-node": "^10.9.2",
    "vercel": "^34.2.7",
    "vite-node": "^2.1.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "dayjs@1.11.13": "patches/dayjs@1.11.13.patch"
    },
    "onlyBuiltDependencies": [
      "@sentry/cli",
      "@swc/core",
      "core-js",
      "esbuild",
      "protobufjs",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f"
}
