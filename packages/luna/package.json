  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist/",
    "build": "pnpm build:ts && pnpm bundle:assets && pnpm build:css",
    "build:ts": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "bundle:assets": "cp src/tailwind.css dist/ && cp tailwind.config.js dist/",
    "watch:pkg": "vite build -w",
    "watch:build": "concurrently --names \"pkg,css\" \"pnpm watch:pkg\" \"pnpm build:css -w\"",
    "build:css": "tailwindcss -m -i src/tailwind.css -o dist/style.css",
    "build:vercel": "pnpm vercel build",
    "deploy:vercel": "pnpm vercel deploy --prebuilt",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "concurrently \"pnpm storybook:css\" \"pnpm storybook:dev\"",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:css": "tailwindcss -w -i src/tailwind.css -o src/style.css",
    "storybook:build:all": "concurrently --names \"css,pkg\" \"pnpm storybook:build:css\" \"pnpm storybook:build\"",
    "storybook:build": "storybook build",
    "storybook:build:css": "tailwindcss -m -i src/tailwind.css -o src/style.css"
  },
